<p id="notice"><%= notice %></p>
<h1>Reservation history</h1>

<table style="border: dotted;margin-left: 20px">
  <thead>
  <tr>
    <th>Start time</th>
    <th>End time</th>
    <th>room number</th>

    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @user.appoints.each do |appoint| %>
      <tr>
        <td><%= appoint.start_time.strftime('%Y-%m-%d %H') %></td>
        <td><%= appoint.end_time.strftime('%Y-%m-%d %H') %></td>
        <% if Room.exists?(id: appoint.room_id)%>
          <td><%= Room.find(appoint.room_id).number %></td>
        <%else%>
            <td><%= "not exists" %></td>
        <%end%>
      </tr>
  <% end %>
  </tbody>
</table>
<p></p>
<div><%= link_to "Back", :back %></div>

