<p id="notice"><%= notice %></p>

<h1>Members</h1>

<p><%= link_to 'Admins', user_list_path %></p>

<table>
  <thead>

  <tr>
    <th>Name</th>
    <th>Email</th>


    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
      <% if user.privilege=='Member'%>
          <tr>
            <td><%= user.name %></td>
            <td><%= user.email %></td>

            <td><%= link_to 'View History', user %></td>
            <td><%= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>

      <%end%>
      </tr>
  <% end %>
  </tbody>
</table>